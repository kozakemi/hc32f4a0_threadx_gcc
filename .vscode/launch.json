{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "HC32F4A0xI Debug (PyOCD)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/hc32f4a0xi-template.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "pyocd",
            "targetId": "hc32f4a0xi",
            "interface": "swd",
            "preLaunchTask": "build",
            "svdFile": "${workspaceFolder}/drivers/HC32F4A0.svd",
            "showDevDebugOutput": "parsed",
            "armToolchainPath": "/usr/bin",
            "gdbPath": "/usr/bin/arm-none-eabi-gdb",
            "loadFiles": [
                "${workspaceFolder}/build/hc32f4a0xi-template.elf"
            ],
            "swoConfig": {
                "enabled": false,
                "cpuFrequency": 240000000,
                "swoFrequency": 4000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "ITM",
                        "port": 0
                    }
                ]
            }
        },
